<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .border {
            margin: 6px;
        }

        .topBorder {
            margin-top: 6px;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" width="350px" height="115px" addStyleNames="{style.border}">
        <g:center>
            <g:FlowPanel>
                <g:Label text="Name:" width="100%"/>
                <g:TextBox ui:field="nameField" width="98%"/>
                <g:Label text="Include Head:" width="100%" addStyleNames="{style.topBorder}"/>
                <g:TextBox ui:field="headerField" width="100%"/>
            </g:FlowPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>
