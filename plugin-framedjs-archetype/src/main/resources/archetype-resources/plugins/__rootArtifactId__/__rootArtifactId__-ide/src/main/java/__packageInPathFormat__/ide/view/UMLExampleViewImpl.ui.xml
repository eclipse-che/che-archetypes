<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>

        .editorView {
            width: 100%;
            height: 100%;
			overflow: auto;
        }

		.editorFrame {
			position: relative;
			border: none;
			width: 100%;
			height: 450px;
		}

    </ui:style>

	<g:FlowPanel width="100%" height="100%" styleName="{style.editorView}">
		<g:Frame ui:field="frame" styleName="{style.editorFrame}"></g:Frame>
	</g:FlowPanel>
</ui:UiBinder>
